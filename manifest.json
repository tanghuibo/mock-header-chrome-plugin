{
    "name": "mock-header-chrome-plugin",
    "version": "0.1",
    "description": "模拟 http 请求头 插件",
    "permissions": [
     "http://*/",
      "https://*/",
      "webRequest",
      "webRequestBlocking"
    ],
    "browser_action": {
      "default_popup": "./pop/index.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "manifest_version": 2
  }
  